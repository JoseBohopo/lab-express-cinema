<div class="container">
<div class=" row">
{{#each movie}}

<div class="col-3">
<div class="card " style="width: 18rem;">
  <img src="{{image}}" class="card-img-top" alt="...">
  <div class="card-body ">
    <h5 class="card-title">{{title}}</h5>
    <p class="card-text">{{director}}</p>
    <a href="movie/{{id}}" class="btn btn-primary">See more</a>
  </div>
</div>
</div>

{{/each}}
</div>
</div>
